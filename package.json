  "description": "AI subagents for specialized coding tasks - delegate work to focused agents for code review, documentation, testing, and debugging",
  "main": "lib/index.js",
  "bin": {
    "cv-agents": "bin/codex-subagents.js",
    "cva": "bin/codex-subagents.js"
  },
  "scripts": {
    "build": "node scripts/build.js",
    "build:dev": "cd rust-cli && cargo build --release",
