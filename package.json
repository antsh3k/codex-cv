  "homepage": "https://github.com/stat-guy/codex-cv",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stat-guy/codex-cv.git"
  },
  "bugs": {
    "url": "https://github.com/stat-guy/codex-cv/issues"
  },
  "engines": {
    "node": ">=16.0.0"
  },
